{
  "version": 6,
  "include": [
    "base.json",
    "compilers/clang.json",
    "triplets/x64-linux.json"
  ],
  "configurePresets": [
    {
      "name": "x64-linux-clang",
      "inherits": [
        "base",
        "clang",
        "x64-linux"
      ]
    }
  ],
  "buildPresets": [
    {
      "name": "x64-linux-clang",
      "inherits": "base",
      "configurePreset": "x64-linux-clang"
    }
  ],
  "testPresets": [
    {
      "name": "x64-linux-clang",
      "inherits": "base",
      "configurePreset": "x64-linux-clang"
    }
  ],
  "workflowPresets": [
    {
      "name": "x64-linux-clang",
      "steps": [
        {
          "type": "configure",
          "name": "x64-linux-clang"
        },
        {
          "type": "build",
          "name": "x64-linux-clang"
        },
        {
          "type": "test",
          "name": "x64-linux-clang"
        }
      ]
    }
  ]
}
